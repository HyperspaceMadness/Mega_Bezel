filter_linear21 = true
scale_type21 = source
scale_x21 = 1.0
scale_y21 = 1.0
alias21 = "PrePass"
mipmap_input21 = true

shader22 = ../../shaders/HyperspaceMadness/hsm-guest/hsm-avg-lum.slang
filter_linear22 = true
scale_type22 = source
scale22 = 1.0
mipmap_input22 = true
alias22 = "AvgLumPass"

// Pass referenced by subsequent blurring passes and crt pass
shader23 = ../../shaders/HyperspaceMadness/hsm-guest/hsm-interlace-and-linearize.slang
filter_linear23 = true
scale_type23 = source
scale23 = 1.0
float_framebuffer23 = true
alias23 = "LinearizePass"


filter_linear18 = true
scale_type18 = source
mipmap_input18 = true
scale18 = 1.0

// Color Correction with Dogway's awesome Grade shader
// Grade is after Afterglow so that brightening the black level does not break the afterglow
shader19 = ../../shaders/HyperspaceMadness/hsm-dogway/hsm-grade.slang
filter_linear19 = true
scale_type19 = source
scale19 = 1.0

// Color Correction with Dogway's awesome Grade shader
shader20 = ../../shaders/HyperspaceMadness/hsm-dogway/hsm-grade.slang


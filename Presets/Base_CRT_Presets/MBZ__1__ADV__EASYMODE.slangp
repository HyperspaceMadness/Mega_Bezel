srgb_framebuffer13 = true

// GTU TV Processing
shader14 = ../../shaders/HyperspaceMadness/hsm-gtu/hsm-gtu-pass1.slang
scale_type14 = source
scale14 = 1.0
float_framebuffer14 = true

shader15 = ../../shaders/HyperspaceMadness/hsm-gtu/hsm-gtu-pass2.slang
scale_type_x15 = source
scale_x15 = 1.0
scale_type_y15 = source
scale_y15 = 1.0
filter_linear15 = false
float_framebuffer15 = true


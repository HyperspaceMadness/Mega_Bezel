shader12 = ../../../shaders/guest/hsm-avg-lum.slang
filter_linear12 = true
scale_type12 = source
scale12 = 1.0
mipmap_input12 = true
alias12 = "AvgLumPass"

# Pass referenced by subsequent blurring passes and crt pass
shader13 = ../../../shaders/guest/hsm-interlace-and-linearize.slang
filter_linear13 = true
scale_type13 = source
scale13 = 1.0
float_framebuffer13 = true
alias13 = "LinearizePass"

// This pass IS ABSOLUTELY NECESSARY
// A pass with mipmap_input = false, and filter_linear = true must be placed after the linearize pass
// Not having this pass results in incorrect sampling later
// Without filter_linear the crt pass has lots of nearest neighbor kind of sampling artifacts
shader14 = ../../../shaders/base/do-nothing.slang
mipmap_input14 = false
filter_linear14 = true
scale_type14 = absolute
scale_x14 = 8
scale_y14 = 8

# Bezel Generation & Composite of Image Layers ----------------------------------------------------------------

shader15 = ../../../shaders/base/bezel-and-image-layers.slang
// Must use nearest neighbor sampling so precision differences don't distort the cached image over time
filter_linear15 = false
scale_type15 = viewport
scale15 = 1.0
alias15 = "SceneLayersPass"

// This pass IS ABSOLUTELY NECESSARY
// A pass with filter_linear = false must be placed after a pass which is packing 4 8 Bit values into a 32 Bit output
// Not having this pass results in incorrect sampling/corrupted values coming from the pass when sampled within another pass
shader16 = ../../../shaders/base/do-nothing.slang
mipmap_input16 = false
filter_linear16 = false
scale_type16 = absolute
scale_x16 = 8
scale_y16 = 8

# Tube Layers ----------------------------------------------------------------

shader17 = ../../../shaders/base/tubelayers-default.slang
// Must use nearest neighbor sampling so precision differences don't distort the cached image over time
filter_linear17 = false
mipmap_input17 = true
scale_type17 = viewport
scale17 = 1.0

// This pass IS ABSOLUTELY NECESSARY
// A pass with filter_linear = false must be placed after a pass which is packing 4 8 Bit values into a 32 Bit output
// Not having this pass results in incorrect sampling/corrupted values coming from the pass when sampled within another pass
shader18 = ../../../shaders/base/do-nothing.slang
mipmap_input18 = false
filter_linear18 = false
scale_type18 = absolute
scale_x18 = 8
scale_y18 = 8

shader19 = ../../../shaders/lcd-cgwg/hsm-lcd-grid-v2.slang
filter_linear19 = "false"
scale_type19 = "viewport"
scale19 = "1.0"
float_framebuffer19 = "true"
alias19 = "CrtPass"


# -----------------------------------------------------------------------------------------------
# LCD-GRID Parameters

# Reduce the Reflection Amount from default
HSM_REFLECT_GLOBAL_AMOUNT = 20

# The LCD-GRID shader expects the color to already have gamma encoded
HSM_NON_USER_LINEARIZE_INTERLACE_PASS = 0

# Remove curvature from the lcd image since it just creates horrible moire
HSM_CRT_CURVATURE_SCALE = 0

# -----------------------------------------------------------------------------------------------

# Reduce Resolution  ----------------------------------------------------------------
#      Reduce the resolution to a small static size regardless of final resolution
#      Allows consistent look and faster at different final resolutions for blur
#      Mipmap option allows downscaling without artifacts
shader20 = ../../../shaders/base/linearize-crt.slang
mipmap_input20 = true
filter_linear20 = true
wrap_mode20 = "mirrored_repeat"
scale_type20 = absolute
scale_x20 = 800
scale_y20 = 600
float_framebuffer20 = true
alias20 = "ReflectPrepLowResPass"

# Add Blur for the Reflection (Horizontal) ----------------------------------------------------------------
shader21 = ../../../shaders/base/blur-outside-screen-horiz.slang
mipmap_input21 = true
wrap_mode21 = "mirrored_repeat"
filter_linear21 = true
float_framebuffer21 = true

# Add Blur for the Reflection (Vertical) ----------------------------------------------------------------
shader22 = ../../../shaders/base/blur-outside-screen-vert.slang
filter_linear22 = true
wrap_mode22 = "mirrored_repeat"
float_framebuffer22 = true
alias22 = "ReflectPrepBlurredPass"

# Reduce resolution ----------------------------------------------------------------
# Reduced to a very small amount so we can create a blur which will create a glow from the screen
#      Mipmap option allows smoother downscaling
shader23 = ../../../../../blurs/shaders/royale/blur9x9.slang
mipmap_input23 = true
filter_linear23 = true
wrap_mode23 = "mirrored_repeat"
scale_type23 = absolute
scale_x23 = 128
scale_y23 = 128
float_framebuffer23 = true
alias23 = "ReflectPrepDiffusedPass"

# Add more blurred glow, used for Bounce Lighting
shader24 = ../../../../../blurs/shaders/royale/blur9x9.slang
mipmap_input24 = true
filter_linear24 = true
wrap_mode24 = "mirrored_repeat"
scale_type24 = absolute
scale_x24 = 32
scale_y24 = 32
float_framebuffer24 = true
alias24 = "BounceLightPass"

# Add Diffused glow all around the screen ----------------------------------------------------------------
#      Blurred so much that it's non directional
#      Mipmap option allows downscaling without artifacts
shader25 = ../../../../../blurs/shaders/royale/blur9x9.slang
mipmap_input25 = true
filter_linear25 = true
wrap_mode25 = "mirrored_repeat"
scale_type25 = absolute
scale_x25 = 12
scale_y25 = 12
float_framebuffer25 = true
alias25 = "ReflectPrepFullscreenGlowPass"

// This pass IS NECESSARY to get the previous pass to be mipmapped
shader26 = ../../../shaders/base/do-nothing.slang
mipmap_input26 = true
filter_linear26 = true
scale_type26 = absolute
scale_x26 = 8
scale_y26 = 8

# Bezel Reflection ----------------------------------------------------------------
shader27 = ../../../shaders/base/reflection.slang
scale_type27 = viewport
filter_linear27 = true
float_framebuffer27 = true
alias27 = "ReflectPass"

# Combine Passes ----------------------------------------------------------------
shader28 = ../../../shaders/base/final-composite.slang
// Must use nearest neighbor sampling so precision differences don't distort the cached image over time
filter_linear28 = false
scale_type28 = viewport


